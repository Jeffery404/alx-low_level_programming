#include "main.h"

/**
 * _isupper - checks for uppercase characters
 *
 * @c: character to check
 *
 * Return: 1 if uppercase, 0 if not 
 */

int _isupper(int c)
{
	if (c >= 'A' && c <= 'Z')
		return (1);
	else 
		return (0);
	_putchar('\n');
}
#include "main.h"

/**
 * _isdigit - checks for digit
 *
 *@c: character to be checked 
 *
 *Return: 1 if digit, 0 otherwise 
 */

int _isdigit(int c)
{
	if (c >= '0' && c <= 9)
		return (1);
	else
		return (0);
	_putchar('\n');
}
#include "main.h"

/**
 * mul - multiplies two integers
 *
 * @a: first number 
 *
 * @b: second number 
 *
 * Return: answer 
 */

int mul (int a, int b)
{
	return (a * b);
} 
#include "main.h"

/**
 * print_numbers - print numbers from 0 to 9
 * return: nothing 
 */

void print_numbers(void)
{
	int a;

	for (a = 0; a <= 9; a++)
	{
		_putchar(a + '0');
	}
	_putchar('\n');
}
#include "main.h"

/**
 * printing_most_numbers - prints digits except 2 and 4
 * Return: nothing 
 */

void print_most_numbers(void)
{
	int a;

	for (a = 0; a <= 9; a++)
	{
		if (a != 2 && a != 4)
			_putchar(a + '0');
	}
	_putchar('\n');
}
#include "main.h"

/**
 * more_numbers - prints 10 times the numbers from 0-14
 * Return: nothing
 */

void more_numbers(void)
{
	int a;
	int b;

	for (b = 0; b < 10; b++)
	{
		if (a > 9)
			_putchar((a / 10) + '0');
		_putchar((a % 10) + '0');
	}
	_putchar('\n');

#include "main.h"

/**
 * print_line - prints line using _
 * @n: number of times _ is printed 
 * Return: nothing 
 */

void print_line(int n)
{
	int a;

	for (a = 0; a < n; a ++)
	{
		_putchar('_');
	}
	_putcha('\n');
}

#include "main.h"

/**
 * printing_diagonal - print \ character on the terminal
 * @n: number of times \ is printed
 * Return: nothing 
 */

void print_diagonal(int n )
{
	int a;
	int b;

	for (a = 0; a < n; a++)
	{
		_putchar(' ');
	}

	_putchar('\\');
	_putchar('\n');
	}
}

#include "main.h"

/**
 * print_square - print squares with #
 * size: size of the square 
 * Return: nothing 
 */

void printing_square(int size)
{
	int a, b;

	for (a = 0; a < size; a++)
	{
		for (b = 0; b < size;
		{
			_putchar('#');
		}
		if (a < size - 1)
			_putchar('\n')
	}
	_putchar('\n');
}

#include <stdio.h>

/**
 * main - print Fizz or Buzz or FizzBuzz
 * Return: 0 Always 
 */

int main(void)
{
	int a;

	for (a = 1; a <= 100; a++)
	{
		if (a % 3 == 0 && a % 5 == 0)
		{
			printf(" FizzBuzz");
		}
		else if (a % 3 == 0)
		{
			printf(" Fizz");
		}
		else if (a % 5 == 0)
		{
			printf(" Buzz");
		}
		else
		{
			printf(" Buzz");
		}
	}
	printf("\n");
	return (0);
}

#include "main.h"

/**
 * print_triangle - prints a triangle
 * @size: number of lines printed 
 * Return: nothing 
 */

void print_triangle(int size)
{
	int a, b;

	for (a = 0; a < size; a++)
	{
		for (b = 1; b < a; b++)
			_putchar(' ');
		for (b--; b < size; b++)
			_putchar('#');
		if (a < (size - 1))
			_putchar('\n');
	}
	_putchar('\n');
}
#include "main.h"
##include <stdio.h>

/**
 * main - prints largest prime factors
 * Return: 0
 */

int main(void)
{
	long int n, fp;

	n = 612852475143;
	for (fp = 2; fp <= n; fp++)
	{
		if (n % fp == 0)
		{
			n /= fp;
			fp--;
		}
	}
	printf("%1d\n", fp);
	return (0);
}

#include "main.h"

/**
 * print_number - prints integer
 * @n: integer to be printed 
 */

void print_number/int n)
{
	if (n < 0)
	{
		_putchar('_');
	}
	if (n / 10)
	{
		print_number(n / 10);
	}
	_putchar((n % 10) + '0');
}

